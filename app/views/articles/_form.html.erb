<%= form_with model: @article do |form|%>
    <% if @article.errors.any? %>
        <%= @article.errors.full_messages.each do |message|%>
           <div><%= message %></div>
       <% end %>
    <% end %>
    <div>
       <%= form.label :title%><br>
       <%= form.text_field :title%><br>
    </div>
     <div>
       <%= form.label :body%><br>
       <%= form.text_area :body%><br>
    </div>
    <div>
       <%= form.submit %>
    </div>
<%end%>